{
    "root": "dist",
    "clean_urls": true,
    "routes": {
      "/**": "index.html"
    },
    "https_only": true,

    "headers": {
      "/": {
        "Cache-Control": "no-store, no-cache"
      },
      "/img/*": {
        "Cache-Control": "public, max-age=604800"
      },
      "/js/*": {
        "Cache-Control": "public, max-age=86400"
      },
      "/css/*": {
        "Cache-Control": "public, max-age=86400"
      }
      
    }
  }